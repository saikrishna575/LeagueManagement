@model LMEntities.Models.TeamManagement

@{
    ViewBag.Title = "Details";
}

<div class="col-md-8 col-sm-6 mininumheight">
    <div id="loginbox" style="margin-top:10px;" class="mainbox col-md-9 col-md-offset-1 col-sm-9">
        @using (Html.BeginForm())
            {
            @Html.AntiForgeryToken()
            <h2>Schedule Details</h2>
            <hr />
            <div class="form-horizontal team">

                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-4 control-label">
                        @Html.DisplayNameFor(model => model.TeamId)
                    </label><div class="col-sm-8">


                        @Html.DisplayFor(model => model.Team.Name)
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-4 control-label">
                        @Html.DisplayNameFor(model => model.TeamMemberId)
                    </label><div class="col-sm-8">


                        @Html.DisplayFor(model => model.TeamMember.Name)
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-4 control-label">
                        @Html.DisplayNameFor(model => model.TitleId)
                    </label><div class="col-sm-8">


                        @Html.DisplayFor(model => model.TeamTitle.Name)
                    </div>
                </div>



                <div class="form-group">

                    <div class="col-md-offset-4 col-md-10">
                        <div class="register">
                            @Html.ActionLink("Back", "Index")
                            @if (User.IsInRole("Admin"))
                            {
                                @Html.ActionLink("Edit", "Edit", new { id = Model.Id })
                            }
                        </div>
                    </div>
                </div>


            </div>
        }
    </div>
</div>

@Html.Partial("_PartialStyle")